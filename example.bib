@article{auster_reference_2004,
	title = {Reference {Librarians} and {Keeping} {Up}-to-{Date}},
	volume = {44},
	issn = {10949054},
	url = {http://search.ebscohost.com/login.aspx?direct=true&db=tfh&AN=15078458&site=ehost-live},
	abstract = {The article looks at the implications of the changing practice of reference work for the professional development of librarians in the twenty-first century. Competencies are the knowledge, skills and personal traits that enable the professional librarian to function effectively in the tasks considered essential in the profession. Communication and interpersonal skills have always been necessary for successful reference work but are seen to be even more critical in today's automated environment. Professional development activities can be characterized as formal or informal types of activities. Librarians seem to prefer informal types of professional development. Deterrents to participation in professional development activities can be characterized as either work-related or non-work-related. Data from the 1994 Adult Education and Training Survey carried out in Canada show that five percent of full-time employees ages twenty to sixty-four did not receive as much training as they were too busy. 733 professional librarians with reference duties who were working in large, urban public libraries in Ontario were surveyed. The Barriers to Participation score ranged from 19 to 86, with a mean score of 51.1 and a median of 52.0. Search strategies for both Internet and electronic resources were also a popular topic as 68 percent of librarians identified this as a topic of study. Just over half the librarians were learning about the integrated library system applications in their libraries.},
	number = {1},
	urldate = {2016-06-02},
	journal = {Reference \& User Services Quarterly},
	author = {Auster, Ethel and Chan, Donna C.},
	year = {2004},
	keywords = {LIBRARIANS, LIBRARY reference services, CAREER development, COMMUNICATION, ABILITY},
	pages = {57--66},
	file = {EBSCO Full Text:/Users/gmorriso/Zotero/storage/IHF5SAZH/Auster and Chan - 2004 - Reference Librarians and Keeping Up-to-Date.pdf:application/pdf}
}

